{
  "name": "multiai",
  "version": "1.0.3",
  "description": "Centro de asistentes de inteligencia artificial",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:linux": "electron-builder --linux deb"
  },
  "author": "StormGamesStudios",
  "homepage": "https://github.com/StormGamesStudios/multiai",
  "license": "MIT",
  "devDependencies": {
    "electron": "^29.1.3",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.stormgamesstudios.multiai",
    "productName": "MultiAI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "src/**/*",
      "assets/**/*",
      "node_modules/**/*",
      "index.html"
    ],
    "linux": {
      "target": [
        "deb"
      ],
      "icon": "assets/icons/",
      "category": "Utility",
      "maintainer": "StormGamesStudios <meliodas.aitor@gmail.com>",
      "vendor": "StormGamesStudios",
      "synopsis": "Aplicaci√≥n de asistentes de IA",
      "description": "Centro de asistentes de inteligencia artificial"
    },
    "deb": {
      "depends": [
        "libgtk-3-0",
        "libnotify4",
        "libnss3",
        "libxss1",
        "libxtst6",
        "xdg-utils"
      ]
    }
  }
}
